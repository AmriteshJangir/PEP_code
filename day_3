//create an array size 32 traverse bits from 0th index and increment by one then add the indexed bits and 
//find common repetations then divide it by 3 and the remaining array is the answer 

#include <iostream>
#include <vector>

int main() {
    const int SIZE = 32;
    std::vector<int> arr(SIZE);
    for (int i = 0; i < SIZE; ++i) {
        arr[i] = i + 1;
    }
    std::cout << "Original Array (first 10 elements):" << std::endl;
    for(int i=0; i<10; ++i) std::cout << arr[i] << " ";
    std::cout << "..." << std::endl;
    for (int i = 0; i < SIZE; ++i) {
        arr[i] = arr[i] / 3;
    }
    std::cout << "\nResulting Array after dividing by 3:" << std::endl;
    for (int i = 0; i < SIZE; ++i) {
        std::cout << arr[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}

//Another:option to go with
#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int *arr=new int[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int result=0;
    int temp[32]={0};
    for(int bit=0;bit<32;bit++){
        int count=0;
        for(int i=0;i<n;i++){
            if(arr[i] & (1<<bit) != 0){
                count++;
            }
        }   
        if(count%3!=0){
            result |= (1<<bit);
        }
    }
    cout<<result;
    delete[] arr;
}
